  "author": "breadhead",
  "scripts": {
    "dev": "cross-env TESTING=true nodemon",
    "dev:debug": "cross-env node --inspect-brk server/index.js",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "test-build": "TESTING=true next build",
    "debug": "node --inspect --inspect-brk=9229 server/index.js",
    "start": "pm2 start ./pm2.json",
    "start-single": "cross-env TESTING=true node ./server/index.js",
    "svg:sprite": "yarn svg-sprite --config ./sprites-config.json  ./public/static/icons/*.svg && node ./sprite-types && prettier --write ./src/core/primitives/Icon/*",
    "test": "jest",
    "pcheck": "yarn types && yarn lint --fix",
    "fullcheck": "yarn types && yarn lint --fix && yarn test",
    "types": "tsc",
    "ci": "yarn lint --fix && yarn test && yarn types && yarn format",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"pages/**/*.{ts,tsx}\"",
    "server:mock": "cd mock-server&& nodemon -r ts-node/register index.ts",
    "server:mock:debug": "cd mock-server&& node --inspect --inspect-brk=9229 -r ts-node/register index.ts",
    "format": "yarn lint --fix && prettier --write --list-different \"src/**/*.{ts,tsx}\" \"pages/**/*.{ts,tsx}\" \"mock-server/**/*.{ts,tsx}\"",
    "test:cypress": "cross-env TESTING=true npm run test-build && cross-env npm run cy:start-server-and-test",
    "test:cypress:dev": "cross-env TESTING=true npm-run-all --parallel --race dev cy:open",
    "test:cypress:ci": "yarn test-build && yarn cy:start-server-and-test:ci",
    "cy:start-server-and-test": "cross-env TESTING=true npx start-server-and-test start-single :3001 cy:run",
    "cy:start-server-and-test:ci": "npx start-server-and-test start-single :3001 cy:run:ci",
    "cy:open": "cross-env cypress open",
    "cy:run": "cross-env cypress run",
    "cy:run:ci": "npx cypress run"
  },
