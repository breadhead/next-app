  "lint-staged": {
    "(src|pages)+/**/*.+(ts|tsx)": [
      "prettier --write --list-different",
      "eslint \"src/**/*.{ts,tsx}\" \"pages/**/*.{ts,tsx}\"",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint --fix"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "publishConfig": {
    "access": "public"
  }
